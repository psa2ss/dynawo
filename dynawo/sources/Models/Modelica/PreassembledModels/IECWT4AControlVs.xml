    <dyn:connect id1="converter" var1="uWtImPu" id2="control" var2="uWtImPu"/>
    <dyn:connect id1="control" var1="ipCmdPu" id2="converter" var2="ipCmdPu"/>
    <dyn:connect id1="control" var1="iqCmdPu" id2="converter" var2="iqCmdPu"/>
    <dyn:connect id1="control" var1="ipMaxPu" id2="converter" var2="ipMaxPu"/>
    <dyn:connect id1="control" var1="iqMaxPu" id2="converter" var2="iqMaxPu"/>
    <dyn:connect id1="control" var1="iqMinPu" id2="converter" var2="iqMinPu"/>
