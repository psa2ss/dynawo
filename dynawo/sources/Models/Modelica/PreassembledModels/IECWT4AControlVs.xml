<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <dyn:modelicaModel id="IECWT4AControl">
    <dyn:unitDynamicModel id="converter" name="Dynawo.Electrical.Sources.WT4AIECelecVs" initName="Dynawo.Electrical.Sources.WT4AIECelecVs_INIT"/>
    <dyn:unitDynamicModel id="control" name="Dynawo.Electrical.Controls.Converters.IECWT4AControlVs" initName="Dynawo.Electrical.Controls.Converters.IECWT4AControlVs_INIT"/>
    <dyn:unitDynamicModel id="omegaRefPu" name="Dynawo.Electrical.Controls.Basics.SetPoint"/>
    <dyn:connect id1="control" var1="omegaRefPu" id2="omegaRefPu" var2="setPoint.value"/>
    <dyn:connect id1="converter" var1="iWtRePu" id2="control" var2="iWtRePu"/>
    <dyn:connect id1="converter" var1="iWtImPu" id2="control" var2="iWtImPu"/>
